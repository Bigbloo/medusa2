# Production environment variables for Render deployment
NODE_ENV=production
MEDUSA_ADMIN_ONBOARDING_TYPE=nextjs

# These will be set by Render automatically
# DATABASE_URL=<set by Render>
# JWT_SECRET=<generated by Render>
# COOKIE_SECRET=<generated by Render>

# CORS settings - will be updated with actual Render URLs
# STORE_CORS=<set by Render>
# ADMIN_CORS=<set by Render>
# AUTH_CORS=<set by Render>

# Optional: Set to true to seed database on first deployment
SEED_DATABASE=true
